        var frmfollow = $('#frmfollow');
        frmfollow.find('.btn-save').on('click', function (e) {
            e.preventDefault();
            var phone = frmfollow.find('#phone').val();
            if (!validatePhoneNumber(phone)) {
                alert('Số điện thoại không hợp lệ.');
                return;
            }
            if (frmfollow.valid()) {
                var data = {
                    roomid: frmfollow.find('#roomid').val(),
                    roomurl: frmfollow.find('#roomurl').val(),
                    CustomerPhone: phone,
                    CustomerName: frmfollow.find('#name').val(),
                    CustomerNotes: frmfollow.find('#note').val(),
                    District: frmfollow.find('#District').val(),
                };

                if (data != null) {
                    $.ajax({
                        url: 'misc/add-follow',
                        type: 'POST',
                        data: JSON.stringify(data),
                        headers: {
                            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                        },
                        contentType: "application/json",  // Sửa lại đúng content type
                        success: function (result) {
                            if (result != null && result['Success']) {
                                alert('Đăng ký tư vấn thành công.');
                                $('#care-about').modal('hide');
                            } else {
                                alert('Đã có lỗi! vui lòng thử lại.');
                                $('#care-about').modal('hide');
                            }
                        },
                        error: function (e) {
                            console.log(e);
                        }
                    });
                }
            }
        });
